{
  "Vendor": true,
  "Deadline": "10m",
  "Sort": ["linter", "severity", "path", "line"],
  "EnableGC": true,
  "WarnUnmatchedDirective": true,
  "DisableAll": true,
  "Linters": {
    "goimports": {
      "Command": "goimports -l --local github.com/Microsoft/presidio"
    }
  },
  "Enable": [
    "deadcode",
    "gofmt",
    "gocyclo",
    "goimports",
    "golint",
    "ineffassign",
    "misspell",
    "vet"
  ],
  "Exclude": ["presidio-api/cmd/presidio-api/docs"]
}
